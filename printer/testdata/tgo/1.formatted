package templates

func test(sth string) {
	<div>"test"</div>
	<div>"test \{sth}"</div>
	<div>
		"test"
	</div>
	<div
		@attr="value"
	>
		"test"
	</div>
	<div
		@attr="value"
	>
		"test \{sth}"
		<span>"test"</span>
		<span
			@attr="value \{sth}"
		>
			"test \{sth}"
			<span>"test"</span>
		</span>
	</div>

	<div>
		"test"
	</div>

	{
		<div>"test"</div>
		<div>"test \{sth}"</div>
		<div
			@attr="value"
			@attr="test \{" " + sth}"
		>
			"\{sth}"
			<span>"\{sth + sth} test"</span>
		</div>
	}

	for _, v := range sth {
		<span>"test"</span>
		<span>"test \{string(v)}"</span>
		<div
			@attr="value"
		>
			for i := range v {
				"test \{i}"
				<span>"test \{i}"</span>
				<span
					@attr="value"
					@attr2="\{i}"
				>
					"test \{i}"
				</span>
			}
			"test \{string(v)}"
		</div>
	}

	if sth == "test" {
		<div>"test div"</div>
		<div>
			"test div"
		</div>
		<div
			@attr="test \{sth}"
		>
			<span>"test div \{sth}"</span>
			<div>
				"test"
				"\{sth}"
				"test"
			</div>
		</div>
	} else if sth == "testing" {
		<div>"testing"</div>
	} else {
		<div>"testing \{sth}"</div>
	}

	switch sth {
	case "test":
		<div>"test"</div>
	default:
		<span>"test"</span>
	}

	<div><span>"test"</span></div>
	<div><span>"test"</span></div>
	<article><div><span>"test \{sth}"</span></div></article>

	<div>
		<article><span>"test"</span></article>
	</div>
}
